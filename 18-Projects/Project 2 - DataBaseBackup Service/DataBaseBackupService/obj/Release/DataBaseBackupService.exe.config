<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Database connection string -->
		<add key="ConnectionString" value="Server=.;Database=StudentsDB;Integrated Security=True;" />

		<!-- Backup folder -->
		<add key="BackupFolder" value="C:\DatabaseBackups" />

		<!-- Backup interval in minutes -->
		<add key="BackupIntervalMinutes" value="1" />

		<!-- Log folder -->
		<add key="LogFolder" value="C:\DatabaseBackups\Logs" />
		
		
	</appSettings>
</configuration>